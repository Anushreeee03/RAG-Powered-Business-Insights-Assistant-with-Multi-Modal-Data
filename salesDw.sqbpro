" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="124"/><column index="2" value="93"/><column index="3" value="62"/><column index="4" value="117"/><column index="5" value="85"/><column index="6" value="81"/><column index="7" value="113"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="stg_sales" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="50"/><column index="2" value="117"/><column index="3" value="85"/><column index="4" value="85"/><column index="5" value="117"/><column index="6" value="78"/><column index="7" value="163"/><column index="8" value="93"/><column index="9" value="109"/><column index="10" value="132"/><column index="11" value="117"/><column index="12" value="73"/><column index="13" value="62"/><column index="14" value="124"/><column index="15" value="124"/><column index="16" value="93"/><column index="17" value="300"/><column index="18" value="78"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">INSERT INTO FactReturns (Order_ID, Product_ID, Customer_ID, Date_ID, Return_Qty, Return_Amount, Reason)
SELECT
    f.Order_ID,
    f.Product_ID,
    f.Customer_ID,
    f.Date_ID,
    1 AS Return_Qty,
    ROUND(f.Sales * (0.2 + (ABS(RANDOM()) % 50) / 100.0), 2) AS Return_Amount,
    CASE ABS(RANDOM()) % 4
        WHEN 0 THEN 'Damaged'
        WHEN 1 THEN 'Customer Dissatisfaction'
        WHEN 2 THEN 'Wrong Item'
        ELSE 'Delivery Delay'
    END AS Reason
FROM FactSales f
WHERE CAST(substr(f.Order_ID, -2) AS INTEGER) % 50 = 0;


CREATE TABLE FactReturns (
    Return_ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Order_ID TEXT,
    Product_ID TEXT,
    Customer_ID TEXT,
    Date_ID TEXT,
    Return_Qty REAL,
    Return_Amount REAL,
    Reason TEXT,
    FOREIGN KEY (Product_ID) REFERENCES DimProduct(Product_ID),
    FOREIGN KEY (Customer_ID) REFERENCES DimCustomer(Customer_ID),
    FOREIGN KEY (Date_ID) REFERENCES DimDate(Date_ID)
);


CREATE TABLE IF NOT EXISTS FactMarketing (
    Campaign_ID INTEGER PRIMARY KEY AUTOINCREMENT,
    Date_ID TEXT,
    Region TEXT,
    Channel TEXT,
    Spend_Amount REAL,
    Campaign_Name TEXT,
    FOREIGN KEY (Date_ID) REFERENCES DimDate(Date_ID)
);


SELECT 
    c.Region,
    SUM(f.Sales) AS Total_Sales,
    SUM(m.Spend_Amount) AS Marketing_Spend
FROM FactSales f
JOIN DimCustomer c ON f.Customer_ID = c.Customer_ID
JOIN FactMarketing m ON f.Date_ID = m.Date_ID AND c.Region = m.Region
GROUP BY c.Region;
</sql><sql name="SQL 2*">DROP TABLE IF EXISTS FactReturns;


INSERT INTO FactMarketing (Date_ID, Region, Channel, Spend_Amount, Campaign_Name)
SELECT DISTINCT
    f.Date_ID,
    c.Region,
    CASE ABS(RANDOM()) % 3
        WHEN 0 THEN 'Digital Ads'
        WHEN 1 THEN 'Email Marketing'
        ELSE 'Influencer Campaign'
    END AS Channel,
    ROUND((ABS(RANDOM()) % 8000) + 2000,2) AS Spend_Amount,
    'Brand Awareness Campaign'
FROM FactSales f
JOIN DimCustomer c ON f.Customer_ID = c.Customer_ID;

SELECT * FROM FactMarketing LIMIT 10;

</sql><current_tab id="0"/></tab_sql></sqlb_project>
